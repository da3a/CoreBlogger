@model IEnumerable<CoreBlogger.Models.Blog>


        @foreach (var blog in Model)
        {
            <div class="col-sm-4 col-lg-4 col-md-4">
                <div class="thumbnail">
                    <img src="blog.ImageThumbnailUrl" alt="">
                    <div class="caption">
                        <h3>
                            <a>@blog.Title</a>
                        </h3>
                        <p>@blog.Owner</p>
                    </div>
                </div>
            </div>        }
